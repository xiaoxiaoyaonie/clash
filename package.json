{
  "name": "clash",
  "version": "1.0.0",
  "description": "一个美观且功能强大的在线工具，用于将各种代理订阅链接转换为 Clash 配置格式",
  "main": "worker.js",
  "scripts": {
    "start": "npx serve .",
    "dev": "python -m http.server 8000",
    "preview": "npx serve .",
    "deploy:worker": "npx wrangler deploy",
    "deploy:pages": "npx wrangler pages deploy . --project-name=clash-subscription-converter",
    "build": "echo 'No build step required for static site'"
  },
  "keywords": [
    "clash",
    "subscription",
    "converter",
    "proxy",
    "v2ray",
    "shadowsocks",
    "trojan",
    "cloudflare-workers"
  ],
